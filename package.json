{
  "name": "kry-react-starter",
  "version": "1.0.0",
  "description": "Starter kit for React",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch start-mockapi",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "local-tunnel": "lt --port 3000 --subdomain kasil",
    "share": "npm-run-all --parallel open:src local-tunnel",
    "test": "mocha --reporter spec buildScripts/testSetup.js src/**/*.test.js src/components/**/*.test.js",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/mockdb.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build:html": "babel-node buildScripts/buildHtml.js",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js",
    "deploy": "surge ./dist"
  },
  "author": "Koray Aşılıoğlu",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "bootstrap": "3.3.7",
    "jquery": "3.2.1",
    "npm": "5.3.0",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-router-redux": "4.0.8",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "toastr": "2.1.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.26.0",
    "chai": "4.1.1",
    "chalk": "2.1.0",
    "cheerio": "1.0.0-rc.2",
    "colors": "1.1.2",
    "compression": "1.7.0",
    "cross-env": "5.0.5",
    "css-loader": "0.28.7",
    "enzyme": "2.9.1",
    "eslint": "3.19.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-react": "7.1.0",
    "eslint-watch": "2.1.14",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.20.2",
    "express": "4.15.4",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.30.1",
    "jsdom": "11.2.0",
    "json-schema-faker": "0.4.3",
    "json-server": "0.12.0",
    "localtunnel": "1.8.3",
    "mocha": "3.5.0",
    "nock": "9.0.14",
    "npm-run-all": "4.1.1",
    "open": "0.0.5",
    "react-test-renderer": "15.6.1",
    "redux-immutable-state-invariant": "2.0.0",
    "redux-mock-store": "1.2.3",
    "rimraf": "2.6.1",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "3.5.5",
    "webpack-dev-middleware": "1.12.0",
    "webpack-hot-middleware": "2.18.2",
    "webpack-md5-hash": "0.0.5"
  }
}
